{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "triple-equals": false,
    "interface-name": false,
    "ordered-imports": false,
    "no-string-literal": false,
    "arrow-parens": false,
    "eofline": false,
    "object-literal-sort-keys": false,
    "object-literal-key-quotes": [true, "as-needed"],
    "no-consecutive-blank-lines": false
  }
}